@model IEnumerable<HusRumFastigheter.Models.FastigheterController>
<h1>FastighetTabel</h1>
<hr />
<p>
    <a asp-action="Create"><button type="button" style ="background-color:#DF8572"><img src="https://img.icons8.com/material-outlined/24/000000/add-property.png"/>Create New</button></a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.DateTime)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Door)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Event)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PersonTag)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Tenant)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Location)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Description)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DateTime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Door.DoorCode)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Event.EventCode)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PersonTag)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Tenant.FullName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Location.Type)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Description)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id"><img src="https://img.icons8.com/office/16/000000/edit.png"/></a> |
                <a asp-action="Details" asp-route-id="@item.Id"><img src="https://img.icons8.com/external-others-phat-plus/16/000000/external-detail-convenience-online-color-line-others-phat-plus.png"/></a> |
                <a asp-action="Delete" asp-route-id="@item.Id"><img src="https://img.icons8.com/plasticine/32/000000/filled-trash.png"/></a>
            </td>
        </tr>
}
    </tbody>
</table>
